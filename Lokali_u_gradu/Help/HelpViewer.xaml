<UserControl x:Class="Lokali_u_gradu.Help.HelpViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Lokali_u_gradu.Help"
             mc:Ignorable="d" 
             d:DesignHeight="430" d:DesignWidth="750">
 
    <DockPanel>
        
        <DockPanel>
            <ToolBar DockPanel.Dock="Top" Height="26">
                <Button x:Name="btnBack" Content="Nazad" Click="btnBack_Click"/>

            </ToolBar>
            <WebBrowser Name="wbHelp"></WebBrowser>
        </DockPanel>
       
    </DockPanel>
</UserControl>
